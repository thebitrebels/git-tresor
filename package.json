{
  "name": "git-tresor",
  "version": "1.0.2",
  "description": "Encrypt and decrypt files to store them inside a git repository.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "decrypt": "node index.js -d -f text.txt.enc",
    "encrypt": "node index.js -e -f text.txt -p hallo123"
  },
  "bin": "index.js",

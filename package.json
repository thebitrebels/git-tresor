  "dependencies": {
    "chalk": "^5.0.1",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "glob": "^8.0.1",
    "inquirer": "^8.2.2",
    "nanospinner": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "8.26.0",
